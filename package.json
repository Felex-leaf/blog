    "{src,config}/**/*.{js,ts,jsx,tsx,json,css,less,scss,html,md}": [
      "prettier --write -l",
      "git add"
    ],
    "{src,config}/**/*.{js,jsx,ts,tsx}": "eslint",
    "*.less": "stylelint"
  },
